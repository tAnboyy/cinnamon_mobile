{
  "expo": {
    "scheme": "cinnamoncustomer",
    "plugins": [
      "@react-native-google-signin/google-signin"
    ],
    "android": {
      "package": "com.anonymous.cinnamoncustomer",
      "googleServicesFile": "./google-services.json"

    },
    "ios": {
      "bundleIdentifier": "com.anonymous.cinnamoncustomer",
      "googleServicesFile": "./GoogleService-Info.plist"

    },
    "extra": {
      "expoPublicGoogleWebClientId": "${EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID}",
      "expoPublicGoogleIosClientId": "${EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID}"
    }
  }
}
